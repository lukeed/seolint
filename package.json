{
  "name": "seolint",
  "version": "0.0.0",
  "repository": "lukeed/seolint",
  "description": "A robust and configurable SEO linter",
  "module": "index.mjs",
  "main": "index.js",
  "license": "MIT",
  "bin": "bin.js",
  "author": "Nick Reese (https://elderguide.com)",
  "contributors": [
    "Luke Edwards <luke.edwards05@gmail.com> (https://lukeed.com)"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "uvu -r ts-node/register -i fixtures test",
    "types": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.d.ts",
    "index.*",
    "bin.js"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "kleur": "^4.1.4",
    "node-html-parser": "^4.1.0"
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "14.14.10",
    "rollup": "2.53.2",
    "terser": "5.7.1",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "uvu": "0.5.1"
  }
}
