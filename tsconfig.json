{
  "compilerOptions": {
    "strict": true,
    "target": "es2019",
    "module": "esnext",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": false,
    "resolveJsonModule": true,
    "esModuleInterop": false,
    "removeComments": true,
    "sourceMap": false,
    "allowJs": false,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "seolint": ["index.d.ts"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
